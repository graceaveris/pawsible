
</br></br>
<div class="container d-flex justify-content-center">
		<div id="show-upload-dimensions">

		  <% if %w(mp4).include?(@upload.media.file.extension.downcase) %>
		  	<video controls="controls" preload="none" width="800" src="<%= @upload.media.url %>" poster="<%= @upload.media.thumb.url %>"></video>
			<% elsif %w(jpg jpeg gif png).include?(@upload.media.file.extension.downcase) %>
		    <%= image_tag @upload.media.url.to_s %>
		    <% else %>
			<% end %>
        </div>
			  

	    <div class="text-left" id="show-page-text">
			<h3><%= @upload.challenge.title %></h3>
			<p class="card-subtitle mb-2 text-muted", style="line-height:18px;">Completed By: <%= @upload.user.dog_name %></p>
			<p  class="card-subtitle mb-2 text-muted", style="line-height:18px;">Owner: <%= @upload.user.owner_name %></p>
			<p  class="card-subtitle mb-2 text-muted", style="line-height:18px;">Challenge Sponsored By: <%= @upload.challenge.sponsor %></p>

			<%= button_to "View Profile", user_path(@upload.user.id), method: :get, class: 'mb-4 btn btn-primary' %>
		      

		</div>

</div>
